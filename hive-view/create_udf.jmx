          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// generate request-flow-id
log.info(&quot;inside Setup - JSR223 PreProcessor&quot;);
function random10(){
	return Math.floor(Math.random() * 9000000000) + 1000000000
}
var requestFlowId = random10();
log.info(&quot;requestFlowId: &quot; + requestFlowId);
vars.put(&quot;requestFlowId&quot;,requestFlowId);
var username = vars.get(&quot;USERNAME&quot;);
log.info(&quot;username : &quot; + username);
var udf_resource_name = username + &quot;_udf_resource_name_&quot; + requestFlowId;
var udf_resource_path = &quot;/some/path/to/udfs/&quot; + udf_resource_name;
var udf_name = username + &quot;_udf_name_&quot; + requestFlowId;
var udf_package = username + &quot;.some.path.pack.&quot; + udf_name;

vars.put(&quot;udf_resource_name&quot;, udf_resource_name);
vars.put(&quot;udf_resource_path&quot;, udf_resource_path);
vars.put(&quot;udf_name&quot;, udf_name);
vars.put(&quot;udf_package&quot;, udf_package);</stringProp>
          <stringProp name="scriptLanguage">javascript</stringProp>
        </JSR223Sampler>
