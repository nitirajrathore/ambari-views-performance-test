              <stringProp name="parameters"></stringProp>
              <stringProp name="script">var response = SampleResult.getResponseDataAsString();
var json = JSON.parse(response);
var jobs = json.jobs;
if(!jobs ){
	AssertionResult.setFailureMessage(&quot;Job array not found. So it failed&quot;);
	AssertionResult.setFailure(true);
}else{
	var job = jobs[0];
	
	vars.put(&quot;job&quot;,job);
	vars.put(&quot;queryFile&quot;, job.queryFile);
	vars.put(&quot;logFile&quot;,job.logFile);
	vars.put(&quot;jobId&quot;,job.id);
}

</stringProp>
