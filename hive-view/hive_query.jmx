              <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="check for job success!" enabled="true">
                <stringProp name="scriptLanguage">javascript</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey"></stringProp>
                <stringProp name="script">var response = prev.getResponseDataAsString();
var json = JSON.parse(response);
vars.put(&quot;job&quot;,json.job);
vars.put(&quot;queryFile&quot;, json.job.queryFile);
vars.put(&quot;logFile&quot;,json.job.logFile);
vars.put(&quot;jobId&quot;,json.job.id);
vars.put(&quot;jobStatus&quot;,json.job.status);
var status = json.job.status;
log.info(&quot;assert while job status : status : &quot; + status); 
if( status == &quot;Error&quot; || status == &quot;Canceled&quot; || status == &quot;Closed&quot; ){
	log.info(&quot;inside if assert while job status : status : &quot; + status); 
	vars.put(&quot;checkForSuccess&quot;,false);	
	AssertionResult.setFailureMessage(&quot;The status for job was : &quot; + status + &quot;. So it failed&quot;);
	AssertionResult.setFailure(true);
} else if (status == &quot;Succeeded&quot; || status == &quot;SUCCEEDED&quot;){ // note sometime the status is capital and sometime its small letter
	vars.put(&quot;checkForSuccess&quot;,false);	
}
</stringProp>
