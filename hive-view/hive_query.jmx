              <stringProp name="parameters"></stringProp>
              <stringProp name="script">var response = SampleResult.getResponseDataAsString();
var json = JSON.parse(response);
var status = json.job.status.toUpperCase();
if( status === &quot;ERROR&quot; || status === &quot;CANCELED&quot; || status === &quot;CLOSED&quot; ){
	vars.put(&quot;checkForSuccess&quot;,false);	
	AssertionResult.setFailureMessage(&quot;The status for job was : &quot; + status + &quot;. So it failed&quot;);
	AssertionResult.setFailure(true);
}else{
	vars.put(&quot;job&quot;,json.job);
	vars.put(&quot;queryFile&quot;, json.job.queryFile);
	vars.put(&quot;logFile&quot;,json.job.logFile);
	vars.put(&quot;jobId&quot;,json.job.id);
	vars.put(&quot;jobStatus&quot;,json.job.status);
	vars.put(&quot;checkForSuccess&quot;,true);	
}

